ost, index) => (\r\n\r\n<div style={{position:'relative',display:'flex',flexDirection:'column'}}>\r\n \r\n \r\n <div class={'buttonColor'} style={{backgroundColor:'rgba(0,0,0,0)',border:0,zIndex:1}} onClick={()=>{\r\n\r\n fetch(process.env.REACT_APP_API+'/userDatta?id='+post.displayName,{method:'GET'})\r\n .then(res=> res.json())\r\n .then(responseJSON =>{\r\n this.setState({\r\n isBioData:responseJSON,\r\n username:post.displayName,\r\n isBio: true,\r\n inRemote:true\r\n \r\n })\r\n })\r\n \r\n \r\n\r\n }} key={index}>\r\n <div style={{alignItems:'center',display:'flex',position:'relative',flexDirection:'column'}}>\r\n \r\n <img style={{width:50,height:50,borderRadius:50,objectFit:'cover'}} src={post.profileImg} />\r\n {post.muted == true? <div  style={{position:'absolute',bottom:0,right:0,alignItems:'center',justifyContent:'center',backgroundColor:'orange',display:'flex',padding:2,borderRadius:30, width:25,height:25}}><i class=\"fa fa-microphone-slash\"></i></div> : <div style={{position:'absolute',bottom:0,display:'flex',right:0,alignItems:'center',justifyContent:'center',backgroundColor:'orange',padding:2,borderRadius:30, width:25,height:25}}><i class=\"fa fa-microphone\"></i></div>}\r\n\r\n </div>\r\n{/*<video style={{display:'none'}}   src={post.stream} />*/}\r\n\r\n </div>\r\n\r\n <button style={{zIndex:2,border:0,marginBlock:5}} onClick={()=>this.onRemoveRequest(post)} class={'beginLiveStreamButtton'} >\r\n <p class={'beginLiveStreampt'}>Remove</p>\r\n </button> \r\n\r\n </div>\r\n    \r\n))}\r\n\r\n </InfiniteScroll>\r\n \r\n \r\n </div>\r\n\r\n\r\n {this.state.requestList.length > 0 ?\r\n <div style={{width:'100%'}}>\r\n <p style={{color:'white'}}>Requesting To Speak {this.state.requestList.length}</p>\r\n <div style={{width:'100%',display:'flex',flexDirection:'row'}}>\r\n \r\n {\r\n \r\n \r\n\r\n <InfiniteScroll\r\n dataLength={this.state.requestList.length}\r\n hasMore={true}\r\n \r\n style={{width:'100%',display:'flex',flexDirection:'row'}} \r\n \r\n >\