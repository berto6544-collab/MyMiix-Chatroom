roup list-group-flush border-bottom scrollarea'}>\r\n    \r\n    {this.state.listMessages.map((posts, index) => (\r\n      <div className={'list-group-item list-group-item-action py-3 lh-tight'} style={{display:'flex',padding:10,position:'relative',alignItems:'center'}} key={index}>\r\n       <div style={{display:'flex',flexDirection:'column',color:'black',wordBreak:'break-word',paddingInline:15}}>\r\n       <div style={{display:'flex',alignItems:'center'}}>\r\n       <img src={posts.userProfile}  style={{width:40,height:40,borderRadius:100,objectFit:'cover'}}/>\r\n        <b>{posts.userName}</b>\r\n        {posts.ismod == true ?<img src={'https://mymiix.com/public/assets/img/mod-badge.png'} style={{width:15,height:15,borderRadius:100}} />:<></>}\r\n        </div>\r\n        {UrlForMedia(posts.message,\"\")}\r\n       </div>\r\n\r\n\r\n       <p style={{position:'absolute',right:5,cursor:'pointer'}} onClick={()=>{\r\n         \r\n         fetch(process.env.REACT_APP_API+'/userDatta?id='+posts.userName,{method:'GET'})\r\n         .then(res=> res.json())\r\n         .then(responseJSON =>{\r\n        if(responseJSON.length > 0){\r\n         this.setState({\r\n           isBioData:responseJSON,\r\n           ShowMore:true,\r\n           MessageLists:this.state.listMessages[index],\r\n           isOnBanList:true\r\n         })\r\n        }\r\n        })\r\n                     \r\n                  }}>More</p>\r\n\r\n\r\n      </div>\r\n    ))}\r\n    \r\n  </div>\r\n\r\n\r\n    </div>\r\n    {this.state.isBanned == false && Utills.getUserType() == \"VIEWER\" || Utills.getUserType() == \"STREAMER\" ?<div class={'d-flex align-items-center flex-shrink-0 p-3 link-dark text-decoration-none border-bottom'} style={{display:'flex',alignItems:'center',width:'100%'}}>\r\n        <textarea\r\n        value={this.state.message} \r\n        ref={this.myRef}\r\n       \r\n        onChange={(e)=>{\r\n          this.setState({message: e.target.value}) \r\n        \r\n        \r\n        }} \r\n        \r\n\r\n        placeholder={\"Post Comment\"}\r\n        style={{width:'100%',fontSize: \"1em\",backgroundColor:'rgb(246, 247, 245)', padding:10,height:100 }}  \r\n        \r\n        />\r\n          \r\n        \r\n        <Button class={'btn btn-primary'} type={'button'}\r\n        \r\n        disabled={this.state.message != \"\"? false : true } \r\n        onClick={()=>{\r\n\r\n\r\n          if(this.props.isRealUser == true){\r\n          if(this.state.message != \"\"){\r\n               SocketUtills.emitSendMessage(this.props.stream,Utills.getUserId(),Utills.getUserNameId(),Utills.getUserProfile(),this.state.message,null,null,null,\"\",this.state.isMod)\r\n\r\n\r\n               \r\n              \r\n               const listMessages = this.state.listMessages;\r\n               const newListMessages = listMessages.slice();\r\n\r\n               newListMessages.push({\r\n              userId:Utills.getUserId(),\r\n              userName:Utills.getUserNameId(),\r\n              userProfile:Utills.getUserProfile(),\r\n              message:this.state.message,\r\n              productId:null,\r\n              productImageUrl:null,\r\n              productUrl:null,\r\n              RequestJoin:\"\",\r\n              ismod:this.state.isMod\r\n              \r\n              })\r\n              this.setState({\r\n                listMessages:newListMessages\r\n              })\r\n\r\n             // this.myRef.current.value = \"\";\r\n             \r\nthis.setState({\r\n  message:\"\"\r\n})\r\nsetTimeout(()=>{\r\nvar objDiv = document.getElementById(\"scrollable1\");\r\n  objDiv.scrollTop = objDiv.scrollHeight;\r\n},150)\r\n          }\r\n        }else{\r\n\r\n          this.setState({\r\n            showSignup:true\r\n          })\r\n        }\r\n\r\n        }}  style={{width:100,height:100}}><span>Post</span></Button>\r\n    </div>:    \r\n    <div class={'d-flex align-items-center flex-shrink-0 p-3 link-dark text-decoration-none border-bottom'} style={{display:'flex',alignItems:'center',justifyContent:'center',width:'100%'}}>\r\n\r\n      <p>Streamer has banned you from their live.</p>\r\n          \r\n    </div>\r\n          \r\n          }\r\n    <div class={'d-flex align-items-center flex-shrink-0 p-3 link-dark text-decoration-none border-bottom'} style={{display:'flex',alignItems:'center',width:'100%'}}>\r\n    \r\n\r\n    {/*<Button onClick={()=>this.setState({showEmoji: !this.state.showEmoji})} variant={\"outline-primary\"} type={'button'} >\r\n    <i style={{color:'yellow'}} class=\"fa fa-tired\"></i></Button>*/}\r\n    \r\n    {Utills.getUserType() == \"STREAMER\"?<Button onClick={()=>this.setState({ShowBanList:true})} variant={\"outline-primary\"} type={'button'} >\r\n    <i class=\"fa fa-ban\"></i></Button>:null}\r\n\r\n\r\n    </div>\r\n\r\n\r\n   \r\n    {this.state.ShowMore == true ?<div style={{width:'100%',height:'100%',display:'flex',position:'absolute',zIndex:400,flexDirection:'column',alignItems:'center',backgroundColor:'white'}}>\r\n    \r\n    <button style={{padding:10,position:'absolute',right:0}} type=\"button\" onClick={()=>{\r\n      this.setState({ShowMore:false,isBioData:[]})\r\n    }} class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\"><span aria-hidden=\"true\">&times;</span></button>\r\n\r\n      \r\n{this.state.isBioData.length > 0 ?\r\n      <div style={{display:'flex',width:'100%',marginTop:30,flexDirection:'column',alignItems:'center',padding:20}}>\r\n       <Avatar src={this.state.isBioData[0].ProfileImage} style={{width:100,height:100}} />\r\n       <b>@{this.state.isBioData[0].UserName}</b>\r\n       {OpenAboutMeUrl(this.state.isBioData[0].AboutMe)}\r\n        {this.state.isBioData[0].LinksArray.length > 0?OpenUrl(this.state.isBioData[0].LinksArray,this.state.isLinkTrue,this.setisLinkTrue):null}\r\n\r\n      </div>\r\n      :<div style={{display:'flex',width:'100%',marginTop:30,flexDirection:'column',alignItems:'center',padding:20}}></div>}\r\n\r\n\r\n    <Button target={'_blank'} onClick={()=>{\r\n     \r\n        \r\n        window.open(\"https://mymiix.com/@\"+this.state.MessageLists.userName+''); \r\n\r\n        this.setState({\r\n          ShowMore:false,\r\n          isBioData:[]\r\n        })\r\n     \r\n      \r\n    }}  class={'btn btn-primary'} type={'button'} style={{width:'100%',zIndex:500,marginTop:20,padding:10}}>Visit Profile</Button>\r\n\r\n\r\n    {Utills.getUserType() == \"STREAMER\" || this.state.isMod == true && Utills.getUserType() == \"VIEWER\" ?<button onClick={()=>{\r\n     \r\n        \r\n        SocketUtills.emitPushToBan(this.props.stream,this.state.MessageLists.userId,this.state.MessageLists.userName,this.state.MessageLists.userProfile)\r\n    \r\n        this.setState({\r\n          ShowMore:false,\r\n          isBioData:[]\r\n        })\r\n     \r\n    }}  class={'btn btn-primary'} type={'button'} style={{width:'100%',marginBlock:5,zIndex:500,padding:10}}>Ban</button>:<></>}\r\n\r\n{Utills.getUserType() == \"STREAMER\"?<button onClick={()=>{\r\n     \r\n        \r\n     SocketUtills.emitPushToMod(this.props.stream,this.state.MessageLists.userId,this.state.MessageLists.userName,this.state.MessageLists.userProfile)\r\n \r\n     this.setState({\r\n       ShowMore:false,\r\n       isBioData:[]\r\n     })\r\n  \r\n }}  class={'btn btn-primary'} type={'button'} style={{width:'100%',marginBlock:5,zIndex:500,padding:10}}>Add {this.state.MessageLists.userName} As Mod</button>:<></>}\r\n\r\n    {Utills.getUserType() == \"STREAMER\"?<button class={'btn btn-primary'} type={'button'} style={{width:'100%',marginBlock:5,zIndex:500,padding:10}} onClick={()=>{\r\n         \r\n        \r\n      \r\n          SocketUtills.emitPinnedComments(this.props.stream,this.state.MessageLists)\r\n          //SocketUtils.emitPinnedComments(Utills.getStreamKey(),this.state.listMessages[index])\r\n        \r\n       this.setState({\r\n         ShowMore:false,\r\n         isBioData:[]\r\n       })\r\n          \r\n  \r\n         }}>Pin</button>:null}\r\n    \r\n    </div>:null}\r\n\r\n\r\n    \r\n    \r\n    </div>\r\n   \r\n\r\n    <Modal\r\n    show={this.state.show}\r\n    onHide={() => this.setState({show:false})}\r\n   \r\n    dialogClassName=\"modal-90w modal-90h my-modal\"\r\n    contentClassName=\"my-modall\"\r\n    style={{backgroundColor:'black',color:'white'}}\r\n\r\n  >\r\n      \r\n    <Modal.Header >\r\n    <button type=\"button\" onClick={()=>{\r\n      this.setState({show:false})\r\n    }} class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\"><span aria-hidden=\"true\">&times;</span></button>\r\n    </Modal.Header>\r\n    <Modal.Body  style={{padding:0, margin:0,width:'100%',height:'100%',}}>\r\n    <div style={{width:'100%',height:'100%',flexDirection:'column',alignItems:'center'}}>\r\n    <iframe  style={{width:'100%',height:'90%',zIndex:100,position:'absolute',border:0}} src={\"https://mymiix.com/public/ReactNativepayment.php?Roomid=\"+Utills.getRoomName()+\"&accountToken=\"+this.state.payment+\"&mode=light&token=\"+Utills.getUserNameId()}   allowFullScreen={true}  ></iframe>\r\n    </div>\r\n    </Modal.Body>\r\n  </Modal>\r\n\r\n\r\n  <Modal\r\n    show={this.state.showSignup}\r\n    onHide={() => this.setState({showSignup:false})}\r\n   \r\n    dialogClassName=\"modal-90w modal-90h my-modal\"\r\n    contentClassName=\"my-modall\"\r\n    style={{backgroundColor:'black',color:'white'}}\r\n\r\n  >\r\n      \r\n    <Modal.Header >\r\n    <button type=\"button\" onClick={()=>{\r\n      this.setState({showSignup:false})\r\n    }} class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\"><span aria-hidden=\"true\">&times;</span></button>\r\n    </Modal.Header>\r\n    <Modal.Body  style={{padding:0, margin:0,width:'100%',height:'100%',}}>\r\n    <div style={{width:'100%',height:'100%',flexDirection:'column',alignItems:'center'}}>\r\n    <iframe  style={{width:'100%',height:'90%',zIndex:100,position:'absolute',border:0}} src={\"https://mymiix.com/signin\"+\"?redirect=https://mymiix.com/@\"+this.props.LiveUserName+'/live'}  allowFullScreen={true}  ></iframe>\r\n    </div>\r\n    </Modal.Body>\r\n  </Modal>\r\n\r\n  \r\n\r\n\r\n  <Modal\r\n    show={this.state.ShowBanList}\r\n    onHide={() => this.setState({ShowBanList:false})}\r\n   \r\n    dialogClassName=\"modal-90w modal-90h my-modal\"\r\n    contentClassName=\"my-modall\"\r\n    style={{backgroundColor:'rgb(30, 144, 255)'}}\r\n\r\n  >\r\n      \r\n    <Modal.Header >\r\n    <button type=\"button\" onClick={()=>{\r\n      this.setState({ShowBanList:false})\r\n    }} class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\"><span aria-hidden=\"true\">&times;</span></button>\r\n    </Modal.Header>\r\n    <Modal.Body  style={{padding:0, margin:0,width:'100%',height:'100%',}}>\r\n    <div style={{width:'100%',height:'100%',display:'flex',flexDirection:'column',alignItems:'center'}}>\r\n    \r\n    <div style={{display:'flex',alignItems:'center',width:'100%',paddingInline:20}}>\r\n    \r\n    <Button target={'_blank'} onClick={()=>{\r\n     \r\n        \r\n    \r\n     this.setState({\r\n      isOnBanList: true \r\n     })\r\n  \r\n   \r\n }}  class={'btn btn-primary'} type={'button'} style={{width:'100%',zIndex:500,marginTop:70,padding:10}}>show Ban List</Button>\r\n\r\n<Button target={'_blank'} onClick={()=>{\r\n     \r\n        \r\n    \r\n     this.setState({\r\n      isOnBanList: false\r\n     })\r\n  \r\n   \r\n }}  class={'btn btn-primary'} type={'button'} style={{width:'100%',zIndex:500,marginTop:70,padding:10}}>show Mod List</Button>\r\n    </div>\r\n\r\n    \r\n    <div id={'scrollable1'} class=\"bg-white\" style={{width:'100%',height:'100%',position:'relative',overflowX:'hidden',overflowY:'scroll',display:'grid', gridTemplateColumns:'auto auto' }}>\r\n    \r\n\r\n\r\n\r\n{this.state.banList.length > 0 && this.state.isOnBanList == true && this.state.banList.map((posts, index) => (\r\n \r\n   \r\n   <div style={{display:'flex',flexDirection:'column',alignItems:'center',pa