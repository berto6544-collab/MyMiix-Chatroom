btn btn-primary'} type={'button'}\r\n        \r\n        disabled={this.state.message != \"\"? false : true } \r\n        onClick={()=>{\r\n\r\n          if(this.state.message != \"\"){\r\n               SocketUtills.emitSendMessage(Utills.getStreamKey(),Utills.getUserId(),Utills.getUserNameId(),Utills.getUserProfile(),this.state.message,null,null,null,\"\",this.state.isMod)\r\n\r\n              ///SocketUtils.emitSendMessage(Utills.getStreamKey(),Utills.getUserId(),Utills.getUserNameId(),Utills.getUserProfile(),this.state.message,null,null,null,\"\")\r\n\r\n               \r\n              \r\n               const listMessages = this.state.listMessages;\r\n               const newListMessages = listMessages.slice();\r\n\r\n               newListMessages.push({\r\n              userId:Utills.getUserId(),\r\n              userName:Utills.getUserNameId(),\r\n              userProfile:Utills.getUserProfile(),\r\n              message:this.state.message,\r\n              productId:null,\r\n              productImageUrl:null,\r\n              productUrl:null,\r\n              RequestJoin:\"\",\r\n              ismod:this.state.isMod\r\n              \r\n              })\r\n              this.setState({\r\n                listMessages:newListMessages\r\n              })\r\n\r\n             // this.myRef.current.value = \"\";\r\n             \r\nthis.setState({\r\n  message:\"\"\r\n})\r\nsetTimeout(()=>{\r\nvar objDiv = document.getElementById(\"scrollable1\");\r\n  objDiv.scrollTop = objDiv.scrollHeight;\r\n},150)\r\n          }\r\n\r\n        }}  style={{width:100,height:100}}><span>Post</span></Button>\r\n    </div>:<div class={'d-flex align-items-center flex-shrink-0 p-3 link-dark text-decoration-none border-bottom'} style={{display:'flex',alignItems:'center',justifyContent:'center',width:'100%'}}>\r\n\r\n<p>Streamer has banned you from their live.</p>\r\n    \r\n</div>}\r\n\r\n\r\n\r\n\r\n    <div class={'d-flex align-items-center flex-shrink-0 p-3 link-dark text-decoration-none border-bottom'} style={{display:'flex',alignItems:'center',width:'100%'}}>\r\n    {Utills.getUserType() == \"VIEWER\"?<Button onClick={()=>this.setState({show:true})} variant={\"outline-primary\"} type={'button'} >\r\n    <i class=\"fa fa-gift\"></i></Button>:<></>}\r\n\r\n    {Utills.getUserType() == \"STREAMER\"?<Button onClick={()=>this.setState({ShowBanList:true})} variant={\"outline-primary\"} type={'button'} >\r\n    <i class=\"fa fa-ban\"></i></Button>:<></>}\r\n\r\n    {/*<Button onClick={()=>this.setState({showEmoji: !this.state.showEmoji})} variant={\"outline-primary\"} type={'button'} >\r\n    <i style={{color:'yellow'}} class=\"fa fa-tired\"></i></Button>*/}\r\n    \r\n    </div>\r\n\r\n\r\n    {this.state.ShowMore == true ?<div style={{width:'100%',height:'100%',display:'flex',position:'absolute',zIndex:400,flexDirection:'column',alignItems:'center',backgroundColor:'white'}}>\r\n    \r\n    <button style={{padding:10,position:'absolute',right:0}} type=\"button\" onClick={()=>{\r\n      this.setState({ShowMore:false,isBioData:[]})\r\n    }} class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\"><span aria-hidden=\"true\">&times;</span></button>\r\n\r\n\r\n{this.state.isBioData.length > 0 ?\r\n      <div style={{display:'flex',width:'100%',marginTop:30,flexDirection:'column',alignItems:'center',padding:20}}>\r\n       <Avatar src={this.state.isBioData[0].ProfileImage} style={{width:100,height:100}} />\r\n       <b>@{this.state.isBioData[0].UserName}</b>\r\n       {OpenAboutMeUrl(this.state.isBioData[0].AboutMe)}\r\n        {this.state.isBioData[0].LinksArray.length > 0? OpenUrl(this.state.isBioData[0].LinksArray,this.state.isLinkTrue,this.setisLinkTrue):null}\r\n\r\n      </div>\r\n      :<div style={{display:'flex',width:'100%',marginTop:30,flexDirection:'column',alignItems:'center',padding:20}} />}\r\n\r\n\r\n    <Button target={'_blank'} onClick={()=>{\r\n     \r\n        \r\n        window.open(\"https://mymiix.com/@\"+this.state.MessageLists.userName+''); \r\n\r\n        this.setState({\r\n          ShowMore:false,\r\n          isBioData:[]\r\n        })\r\n      \r\n    }}  class={'btn btn-primary'} type={'button'} style={{width:'100%',zIndex:500,marginTop:20,padding:10}}>Visit Profile</Button>\r\n\r\n\r\n    {Utills.getUserType() == \"STREAMER\" || this.state.isMod == true && Utills.getUserType() == \"VIEWER\"?<button onClick={()=>{\r\n     \r\n        \r\n        SocketUtills.emitPushToBan(Utills.getStreamKey(),this.state.MessageLists.userId,this.state.MessageLists.userName,this.state.MessageLists.userProfile)\r\n    \r\n        this.setState({\r\n          ShowMore:false,\r\n          isBioData:[]\r\n        })\r\n     \r\n    }}  class={'btn btn-primary'} type={'button'} style={{width:'100%',marginBlock:5,zIndex:500,padding:10}}>Ban</button>:<></>}\r\n\r\n\r\n{Utills.getUserType() == \"STREAMER\"?<button onClick={()=>{\r\n     \r\n        \r\n     SocketUtills.emitPushToMod(Utills.getStreamKey(),this.state.MessageLists.userId,this.state.MessageLists.userName,this.state.MessageLists.userProfile)\r\n \r\n     this.setState({\r\n       ShowMore:false,\r\n       isBioData:[]\r\n     })\r\n  \r\n }}  class={'btn btn-primary'} type={'button'} style={{width:'100%',marginBlock:5,zIndex:500,padding:10}}>Add {this.state.MessageLists.userName} As Mod</button>:<></>}\r\n\r\n    {Utills.getUserType() == \"STREAMER\"?<button class={'btn btn-primary'} type={'button'} style={{width:'100%',marginBlock:5,zIndex:500,padding:10}} onClick={()=>{\r\n         \r\n        \r\n      \r\n          SocketUtills.emitPinnedComments(Utills.getStreamKey(),this.state.MessageLists)\r\n         \r\n        \r\n          this.setState({\r\n            ShowMore:false,\r\n            isBioData:[]\r\n          })\r\n          \r\n  \r\n         }}>Pin</button>:null}\r\n    \r\n    </div>:null}\r\n\r\n    </div>\r\n   \r\n\r\n    <Modal\r\n    show={this.state.show}\r\n    onHide={() => this.setState({show:false})}\r\n   \r\n    dialogClassName=\"modal-90w modal-90h my-modal\"\r\n    contentClassName=\"my-modall\"\r\n    style={{backgroundColor:'black',color:'white'}}\r\n\r\n  >\r\n      \r\n    <Modal.Header >\r\n    <button type=\"button\" onClick={()=>{\r\n      this.setState({show:false})\r\n    }} class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\"><span aria-hidden=\"true\">&times;</span></button>\r\n    </Modal.Header>\r\n    <Modal.Body  style={{padding:0, margin:0,width:'100%',height:'100%',}}>\r\n    <div style={{width:'100%',height:'100%',flexDirection:'column',alignItems:'center'}}>\r\n    <iframe  style={{width:'100%',height:'90%',zIndex:100,position:'absolute',border:0}} src={\"https://mymiix.com/public/ReactNativepayment.php?Roomid=\"+Utills.getRoomName()+\"&accountToken=\"+this.state.payment+\"&mode=light&token=\"+Utills.getUserNameId()}  frameBorder={0} allowFullScreen={true}   ></iframe>\r\n    </div>\r\n    </Modal.Body>\r\n  </Modal>\r\n\r\n\r\n  <Modal\r\n    show={this.state.ShowBanList}\r\n    onHide={() => this.setState({ShowBanList:false})}\r\n   \r\n    dialogClassName=\"modal-90w modal-90h my-modal\"\r\n    contentClassName=\"my-modall\"\r\n    style={{backgroundColor:'rgb(30, 144, 255)',color:'white'}}\r\n\r\n  >\r\n      \r\n    <Modal.Header >\r\n    <button type=\"button\" onClick={()=>{\r\n      this.setState({ShowBanList:false})\r\n    }} class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\"><span aria-hidden=\"true\">&times;</span></button>\r\n    </Modal.Header>\r\n    <Modal.Body  style={{padding:0, margin:0,width:'100%',height:'100%',}}>\r\n    <div style={{width:'100%',height:'100%',display:'flex',justifyContent:'center',flexDirection:'column',alignItems:'center'}}>\r\n    \r\n    <div style={{display:'flex',alignItems:'center',width:'100%',paddingInline:20}}>\r\n    \r\n    <Button  onClick={()=>{\r\n     \r\n        \r\n    \r\n     this.setState({\r\n      isOnBanList: true \r\n     })\r\n  \r\n   \r\n }}  class={'btn btn-primary'} type={'button'} style={{width:'100%',zIndex:500,marginTop:70,padding:10}}>show Ban List</Button>\r\n\r\n<Button target={'_blank'} onClick={()=>{\r\n     \r\n        \r\n    \r\n     this.setState({\r\n      isOnBanList: false\r\n     })\r\n  \r\n   \r\n }}  class={'btn btn-primary'} type={'button'} style={{width:'100%',zIndex:500,marginTop:70,padding:10}}>show Mod List</Button>\r\n    </div>\r\n\r\n    <div id={'scrollable1'} class=\"bg-white\" style={{width:'100%',height:'100%',position:'relative',overflow: 'auto',flexDirection: 'column-reverse', }}>\r\n\r\n\r\n<div  class={'list-group list-group-flush border-bottom scrollarea'}>\r\n\r\n{this.state.banList.length > 0 && this.state.isOnBanList == true && this.state.banList.map((posts, index) => (\r\n \r\n   \r\n <div style={{display:'flex',flexDirection:'column',alignItems:'center',padding:10 }} key={index}>\r\n <img src={posts.userProfile}  style={{width:40,height:40,borderRadius:100,objectFit:'cover'}}/>\r\n  <b>{posts.userName}</b>\r\n  <button onClick={()=>{\r\n \r\n  SocketUtills.emitUnBan(Utills.getStreamKey(),posts.userId,posts.userName,posts.userProfile)\r\n\r\n  \r\n\r\n  }} class={'btn btn-primary'} style={{zIndex:500}} type={'button'}>UnBan</button>\r\n  </div>\r\n\r\n\r\n\r\n\r\n)) }\r\n\r\n{\r\nthis.state.modList.length > 0 && this.state.isOnBanList == false && this.state.modList.map((posts, index) => (\r\n\r\n \r\n  <div style={{display:'flex',flexDirection:'column',alignItems:'center',padding:10 }} key={index}>\r\n  <img src={posts.userProfile}  style={{width:40,height:40,borderRadius:100,objectFit:'cover'}}/>\r\n   <b>{posts.userName}</b>\r\n   <button onClick={()=>{\r\n  \r\n   SocketUtills.emitUnMod(Utills.getStreamKey(),posts.userId,posts.userName,posts.userProfile)\r\n\r\n   \r\n\r\n   }} class={'btn btn-primary'} style={{zIndex:500}} type={'button'}>UnMod</button>\r\n   </div>\r\n \r\n\r\n\r\n\r\n))\r\n}\r\n\r\n\r\n\r\n</div>\r\n\r\n\r\n</div>\r\n   \r\n    \r\n    </div>\r\n\r\n\r\n    \r\n    </Modal.Body>\r\n  </Modal>\r\n\r\n\r\n\r\n  \r\n\r\n    </div>\r\n)\r\n    }\r\n\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\nfunction OpenAboutMeUrl(url){\r\n       \r\n\r\n        \r\n  if(url != undefined){\r\n  \r\n  if (typeof(url === 'string')) {\r\n  \r\n    // Split the content on space characters\r\n    var words = url.toString().split(/\\s/);\r\n  \r\n    // Loop through the words\r\n    var contents = words.map(function(word, i) {\r\n  \r\n      // Space if the word isn't the very last in the set, thus not requiring a space after it\r\n      var separator = i < (words.length - 1) ? ' ' : '';\r\n  \r\n      // The word is a URL, return the URL wrapped in a custom <Link> component\r\n      if (word.match(/^https?\\:\\//)) {\r\n  //  return <Text style={{color:'blue'}} onPress={()=>handlePress(word)} >{word}{separator}</Text>;\r\n      // The word is not a URL, return the word as-is\r\n      } \r\n      \r\n      \r\n      \r\n      else {\r\n\r\n        return word+separator;\r\n      }\r\n    });\r\n\r\n    \r\n\r\n  \r\n    \r\n    \r\n  \r\n  // The nested content was something else than a plain string\r\n  // Return the original content wrapped in a <Text> component\r\n  } else {\r\n    return url.toString();\r\n  }\r\n  \r\n  \r\n  return (\r\n    \r\n      <div style={{alignItems:'center'}}>\r\n        <p style={{width:'100%',textAlign:'center',marginBottom:10}}>{contents}</p>\r\n       \r\n        </div>\r\n        \r\n    \r\n  );\r\n\r\n  }\r\n  \r\n  \r\n  }\r\n\r\nfunction OpenUrl(url,isTrue,setisTrue){\r\n       \r\n\r\n        \r\n  if(url.length > 0){\r\n  \r\n  \r\n  \r\n    // Split the content on space characters\r\n   \r\n\r\n    \r\n\r\n\r\n\r\n\r\n\r\n\r\n    var contentss = url.map(function(word, i) {\r\n  \r\n      \r\n      \r\n      \r\n      if (word.Link.match(/http(?:s)?:\\/\\/(?:www\\.)?twitter\\.com\\/([a-zA-Z0-9_]+)/)) {\r\n        return <a href={word.Link} style={{flexDirection:'row',width:'100%',position:'relative',textDecoration:'none',padding:10,borderRadius:10,marginBottom:10,backgroundColor:'lightgrey',display:'flex',alignItems:'center'}}>\r\n         <img width={30} height={30} style={{marginRight:5,objectFit:'contain'}} src={'https://www.iconpacks.net/icons/2/free-twitter-logo-icon-2429-thumb.png'} />\r\n           {word.LinkText}\r\n           <i style={{position:'absolute',right:10}} class=\"fa fa-chevron-right\"></i>\r\n\r\n           </a>\r\n          ;\r\n\r\n        // The word is not a URL, return the word as-is\r\n        } \r\n\r\n        else if (word.Link.match(/http(?:s)?:\\/\\/(?:www\\.)?twitch\\.tv\\/([a-zA-Z0-9_]+)/)) {\r\n          return <a href={word.Link} style={{flexDirection:'row',width:'100%',width:'100%',position:'relative',overflow:'hidden',width:'100%',display:'flex',textDecoration:'none',padding:10,borderRadius:10,marginBottom:10,backgroundColor:'lightgrey',display:'flex',alignItems:'center'}}>\r\n            <img width={30} height={30} style={{marginRight:5,objectFit:'contain'}} src={'https://assets.stickpng.com/images/580b57fcd9996e24bc43c540.png'} />\r\n            {word.LinkText}\r\n            <i style={{position:'absolute',right:10}} class=\"fa fa-chevron-right\"></i>\r\n\r\n            </a>\r\n            \r\n             ;\r\n          // The word is not a URL, return the word as-is\r\n          } \r\n\r\n          else if (word.Link.match(/http(?:s)?:\\/\\/(?:www\\.)?youtube\\.com\\/([a-zA-Z0-9_]+)/)) {\r\n              return <a href={word.Link} style={{flexDirection:'row',width:'100%',position:'relative',textDecoration:'none',padding:10,borderRadius:10,marginBottom:10,backgroundColor:'lightgrey',display:'flex',alignItems:'center'}}>\r\n                <img width={30} height={30} style={{marginRight:5,objectFit:'contain'}} src={'https://www.iconpacks.net/icons/2/free-youtube-logo-icon-2431-thumb.png'} />\r\n                {word.LinkText}\r\n                <i style={{position:'absolute',right:10}} class=\"fa fa-chevron-right\"></i>\r\n                </a>;\