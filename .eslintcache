EACT_APP_API+'/userDatta?id='+item.displayName,{method:'GET'})\r\n.then(res=> res.json())\r\n.then(responseJSON =>{\r\nthis.setState({\r\nisBioData:responseJSON,\r\nusername:item.displayName,\r\nisBio: true,\r\ninRemote:false\r\n\r\n})\r\n\r\n})\r\n\r\n\r\n\r\n}} style={{width:50,height:50,borderRadius:50,position:'relative'}}>\r\n\r\n<img style={{width:50,height:50,borderRadius:50,objectFit:'cover'}} src={item.profileImg} />\r\n\r\n</div>\r\n<button style={{zIndex:2,border:0,marginBlock:5}} onClick={()=>this.onRequestToBeLivve(item)} class={'beginLiveStreamButtton'}>\r\n<p class={'beginLiveStreampt'} >Request</p>\r\n</button> \r\n\r\n</div>\r\n\r\n))}\r\n\r\n\r\n </InfiniteScroll>\r\n\r\n \r\n :<></>\r\n }\r\n </div>\r\n </div>: \r\n <div style={{width:'100%',position:'relative',display:'flex',alignItems:'center'}}>\r\n   <a style={{color:'white',fontWeight:'bold'}} href={this.state.streamUrl}>Tap to listen to your stream</a>\r\n  \r\n   \r\n   \r\n   </div>}\r\n\r\n <div class={'containerr'}>\r\n \r\n \r\n \r\n \r\n \r\n\r\n \r\n \r\n\r\n {/*this.renderVideo()*/}\r\n \r\n \r\n\r\n \r\n \r\n </div>\r\n \r\n \r\n\r\n \r\n \r\n \r\n\r\n <div class={'wrapBottomm'}>\r\n \r\n <button\r\n class={'wrapIconnSend'}\r\n style={{position:'relative'}}\r\n onClick={()=>this.setState({isMessaging:true})}\r\n activeOpacity={0.6}\r\n >\r\n <img\r\n src={'https://mymiix.com/assets/ico_send.png'}\r\n class={'iconSend'}\r\n style={{position:'absolute'}}\r\n />\r\n <p style={{fontWeight:'800',zIndex:40}}>{formatToUnits(this.state.listMessages.length)}</p>\r\n\r\n </button>\r\n \r\n {this.state.UserType == \"STREAMER\" ?<button\r\n class={'wrapIconnSend'}\r\n style={{position:'relative'}}\r\n onClick={()=>this.setState({ShowBanList:true})}\r\n activeOpacity={0.6}\r\n >\r\n<i style={{fontSize:24,color:'white'}} class=\"fa fa-ban\"></i>\r\n\r\n\r\n </button>:null}\r\n \r\n <button\r\n class={'wrapIconnSend'}\r\n style={{position:'relative'}}\r\n onClick={()=>{\r\n  this.setState({\r\n   